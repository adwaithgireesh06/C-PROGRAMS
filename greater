#include<stdio.h>
int main()
 {
 int a,b;
 printf("TO FIND THE GREATER NUMBER\n");
 printf("--------------------------\n");
 printf("Enter a Number : ");
 scanf("%d",&a);
  printf("Enter another Number : ");
  scanf("%d",&b);
int g=(a>b)?a:b;
printf("The Greatest of These Numbers is :%d",g);
 return 0;
 }
